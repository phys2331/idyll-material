             [Boolean value:fqe /] [Equation]\vec{F} = q\vec{E}[/Equation] [br /]
             [Boolean value:FKin /] [Equation]\vec{x}(t) = \vec{x}_{0} + \vec{v}_{0}t + \frac{1}{2}\vec{a}t^{2}[/Equation] [br /]

             [var name:"fma_ans" value:false /]
