             [Radio value:radioValQ1_1 onClick:`showResult1434_1=false,showAnswer1434_1=false` options:`[{value:"b",label:"b) Charge A could be positive and charge B could be negative."}]`/]
             [Radio value:radioValQ1_1 onClick:`showResult1434_1=false,showAnswer1434_1=false` options:`[{value:"c",label:"c) Charge A could be negative and charge B could be positive."}]`/]
             [Radio value:radioValQ1_1 onClick:`showResult1434_1=false,showAnswer1434_1=false` options:`[{value:"d",label:"d) Charges A and B could both be negative."}]`/]

             [Button onClick:`showResultQ1_1=true`]
